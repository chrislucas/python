import sys
import __main__


def testIO():
    while True:
        from sys import stdout, stdin
        n = stdin
        try:
            n = int(n)
            stdout.write(str(n & 1))
        except:
            print "erro ao executar metodo de convercao para inteiro"
            break
       
    
if __name__ == "__main__":
    testIO()